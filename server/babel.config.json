{
  "presets": [
    "@babel/preset-env",
    ["@babel/preset-typescript", { "allowDeclareFields": true }]
  ],
  "plugins": [
    ["@babel/plugin-transform-typescript", { "allowDeclareFields": true }],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}
